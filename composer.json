{
  "name": "mforest-gsu/d2l-datahub-cli",
  "license": "MIT",
  "homepage": "https://github.com/mforest-gsu",
  "require": {
    "php": ">=8.1",
    "ext-dom": "*",
    "mjfk23/console": "^1.0",
    "mjfk23/d2l-api": "^1.0",
    "mjfk23/httpclient": "^1.0",
    "mjfk23/pdo": "^1.0",
    "guzzlehttp/guzzle": "^7.7",
    "guzzlehttp/psr7": "^2.5"
  },
  "require-dev": {
    "phpstan/extension-installer": "^1.1",
    "phpstan/phpstan": "^1.9",
    "phpstan/phpstan-strict-rules": "^1.3",
    "squizlabs/php_codesniffer": "^3.7"
  },
  "config": {
    "allow-plugins": {
      "phpstan/extension-installer": true
    }
  },
  "autoload": {
    "psr-4": {
      "D2L\\DataHub\\": "src/"
    }
  },
  "scripts": {
    "phpcs": "vendor/bin/phpcs",
    "phpstan": "vendor/bin/phpstan analyse -c phpstan.neon",
    "console": "bin/console"
  }
}
